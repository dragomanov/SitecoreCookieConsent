<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      
      <!-- Web Forms pipeline processor -->
      <renderLayout>
        <processor type="Sitecore.CookieConsent.Pipelines.RenderLayout.RenderCookieConsent, Sitecore.CookieConsent" />
      </renderLayout>
      
      <!-- Mvc pipeline processor -->
      <mvc.requestEnd>
        <processor type="Sitecore.CookieConsent.Pipelines.RequestEnd.RenderCookieConsent, Sitecore.CookieConsent" />  
      </mvc.requestEnd>
      
    </pipelines>
  </sitecore>
</configuration>
